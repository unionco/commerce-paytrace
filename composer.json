{
    "name": "unionco/commerce-paytrace",
    "description": "Craft Commerce 2 Payment Gateway for PayTrace",
    "type": "craft-plugin",
    "version": "0.0.1",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "commerce-paytrace"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "UNION",
            "homepage": "https://github.com/unionco"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0-RC1",
        "craftcms/commerce": "^2.2",
        "unionco/omnipay-paytrace": "@dev",
        "craftcms/commerce-omnipay": "@dev",
        "omnipay/common": "^3",
        "php-http/guzzle6-adapter": "^1.1.1",
        "php-http/httplug": "^1"
    },
    "autoload": {
        "psr-4": {
          "unionco\\paytrace\\": "src/"
        }
    },
    "extra": {
        "name": "commerce-paytrace",
        "handle": "commerce-paytrace",
        "hasCpSettings": false,
        "hasCpSection": false,
        "class": "unionco\\paytrace\\PayTrace"
    }
}
